{
  "name": "@rotsen/vue-filerobot-image-editor",
  "description": "Filerobot-image-editor from vuejs",
  "version": "0.1.1",
  "author": "Nestor Koueya <koueya@gmail.com>",
  "main": "dist/vue-filerobot-image-editor.umd.js",
  "module": "dist/vue-filerobot-image-editor.esm.js",
  "unpkg": "dist/vue-filerobot-image-editor.min.js",
  "browser": {
    "./sfc": "src/index.js"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "yarn build:umd & yarn  build:es & yarn build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-filerobot-image-editor.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-filerobot-image-editor.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-filerobot-image-editor.min.js"
  },
  "peerDependencies": {
    "vue": ">=2"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "11.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "core-js": "3.6.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "prettier": "^2.2.1",
    "prettier-linter-helpers": "^1.0.0",
    "rollup": "1.17.0",
    "rollup-plugin-vue": "5.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "vue-eslint-parser": "7.6.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koueya/vue-filerobot-image-editor.git"
  },
  "keywords": [
    "Vue",
    "Filerobot-image"
  ],
  "bugs": {
    "url": "https://github.com/koueya/vue-filerobot-image-editor/issues"
  },
  "homepage": "https://github.com/koueya/vue-filerobot-image-editor#readme"
}
