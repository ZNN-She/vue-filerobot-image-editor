{
  "ignorePatterns": [
    "filerobot-image-editor.min.js",
    "*.vue"
  ],
  "extends": [
    "prettier",
    "plugin:prettier/recommended",
    "plugin:vue/recommended",
    "plugin:vue/base"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "jsx": false,
      "modules": false
    }
  }
}
